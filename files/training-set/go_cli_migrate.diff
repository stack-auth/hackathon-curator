package main

import (
  "database/sql"
  "fmt"
  _ "modernc.org/sqlite"
)

func main(){
  db, _ := sql.Open("sqlite", "file:data.db")
  db.Exec("CREATE TABLE IF NOT EXISTS users(id INTEGER PRIMARY KEY, name TEXT)")
  for i:=1; i<=10; i++{
    db.Exec(fmt.Sprintf("INSERT INTO users(name) VALUES('user_%d')", i))
  }
}

